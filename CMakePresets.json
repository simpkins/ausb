{
  "version": 3,
  "cmakeMinimumRequired": {
   "major": 3,
   "minor": 19,
   "patch": 0
  },
  "configurePresets": [
    {
      "name": "debug",
      "generator": "Ninja",
      "binaryDir": "build/debug",
      "cacheVariables": {
	"CMAKE_BUILD_TYPE": "Debug"
      },
      "warnings": {
	"dev": true,
	"uninitialized": true
      }
    },
    {
      "name": "release",
      "generator": "Ninja",
      "binaryDir": "build/release",
      "cacheVariables": {
	"CMAKE_BUILD_TYPE": "Release"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "debug",
      "configurePreset": "debug"
    },
    {
      "name": "release",
      "configurePreset": "release"
    }
  ]
}
